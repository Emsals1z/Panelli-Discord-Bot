   <div class="row layout-top-spacing" id="cancel-row">
         
      <div class="col-xl-4 col-lg-12 col-md-12 col-sm-12 col-12 layout-spacing">
         <div class="widget widget-one">
            <br>
    <label><i class="fad fa-tags" style="color: #696969"></i style="color: #696969"> Çekiliş</label>
   <br>
            <br>
      <div class="input-group mb-2 mr-sm-2 mb-sm-0" >
            <div class="input-group-addon" style="width: 50%; text-align: left; margin: 0; ">Kazanan Sayısı</div>
            <% if (db.has(`çekiliş_${guild.id}.winner`) === false) { %>
     <input type="text" class="form-control u-btn-round" name="çw" id="çw" placeholder="2">
         <% } %>
        <% if (db.has(`çekiliş_${guild.id}.winner`) === true) { %>
       <input type="text" class="form-control u-btn-round" name="çw" id="çw" placeholder="<%=db.get(`çekiliş_${guild.id}.winner`)%>">
         <% } %>
      </div>
    <div class="input-group mb-2 mr-sm-2 mb-sm-0" >
            <div class="input-group-addon" style="width: 50%; text-align: left; margin: 0;">Ödül</div>
            <% if (db.has(`çekiliş_${guild.id}.prize`) === false) { %>
     <input type="text" class="form-control u-btn-round" name="çö" id="çö" placeholder="1 Ay Nitro">
         <% } %>
        <% if (db.has(`çekiliş_${guild.id}.prize`) === true) { %>
       <input type="text" class="form-control u-btn-round"  name="çö" id="çö" placeholder="<%=db.get(`çekiliş_${guild.id}.prize`)%>">
         <% } %>
      </div>
    <div class="input-group mb-2 mr-sm-2 mb-sm-0" >
            <div class="input-group-addon" style="width: 50%; text-align: left; margin: 0; ">Zaman(s,m,h,d)</div>
            <% if (db.has(`çekiliş_${guild.id}.time`) === false) { %>
     <input type="text" class="form-control u-btn-round" name="çt" id="çt" placeholder="5m">
         <% } %>
        <% if (db.has(`çekiliş_${guild.id}.time`) === true) { %>
       <input type="text" class="form-control u-btn-round"name="çt" id="çt" placeholder="<%=db.get(`çekiliş_${guild.id}.time`)%>">
         <% } %>
      </div>
   <div class="input-group mb-2 mr-sm-2 mb-sm-0" style="margin: 0">
          <div class="select">    
         <select d="çk" name="çk">
            <% if (db.has(`çekiliş_${guild.id}.channel`) === false) { %>
       <option disabled selected> Çekiliş Kanal </option><br>
             <% } %>
            <% if (db.has(`çekiliş_${guild.id}.channel`) === true) { %>
            <option disabled selected> Çekiliş Kanal : <%= çekilişlog%> </option>
             <% } %>
            <% const kanallar = guild.channels.cache.sort((a, b) => a.position - b.position) %>
              <% kanallar.map(item=> { %>
              <% if (item.type == 'voice' || item.type == 'category') return;%>
              <option value="<%=item.id%>" id="<%= item.id%>"> <%= item.name %> </option>
              <% }) %>
            </select>
           </div>
           </div>
           <br>
    <br>
      <div style="width: 100%; text-align: left; margin-top: -5px;">
        	 <a class="u-btn-round"><a class="btn btn-primary u-btn-round" href="/dashboard/<%= guild.id %>/fun/cekilis">Çekilişi başlatmak için tıkla</a></a>
        	 <a class="u-btn-round"><a class="btn btn-primary u-btn-round" href="/dashboard/<%= guild.id %>/fun/cekilis/reset">Sıfırla</a></a>
      </div>
  
   <br>
   <br>
             </div>
        </div>
       
     
         
     
           
      
      <div style="width: 100%; text-align: right; margin-top: 20px;">
         <a  type="submit" class="btn btn-primary u-btn-round"    onclick="window.location.href='/dashboard/<%= guild.id %>/manage'"><i class="fa fa-reply "></i> Geri<i aria-hidden='true'></i></a>
        				<a  type="submit" class="btn btn-danger u-btn-round"  onclick="myFunction2()"><i class="fa fa-fw fa-sign-out"></i> Botu At<i aria-hidden='true'></i></a>
<a type="submit" class="btn btn-primary u-btn-round"  onclick="myFunction1()"><i class="fa fa-fw fa-cogs" aria-hidden="true"></i>Ayarları Sıfırla</a>
					<button  type="submit"class="btn btn-primary u-btn-round" onclick="myFunction()"><i class="fa fa-save fa-save"></i> Ayarları Kaydet<i aria-hidden='true'></i></button>
        
      </div>
