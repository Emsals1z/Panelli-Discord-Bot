    </form>
 <script>
                 var urlParams = new URLSearchParams(window.location.search);
           function myFunction() {
                    Swal.fire({
               toast: true,
  position: 'top-end',
  showConfirmButton: false,
  timer: 750,
  timerProgressBar: true,
  didOpen: (toast) => {
    toast.addEventListener('mouseenter', Swal.stopTimer)
    toast.addEventListener('mouseleave', Swal.resumeTimer)
  },
 
  icon: 'success',
  title: 'Ayarlarınız kaydedildi.'
 
})
     
            }
             function myFunction1() {
          Swal.fire({
  title: 'Emin misin?',
  text: "Eğer sıfırlarsan geri dönüşü olmaz.",
  icon: 'warning',
  showCancelButton: true,
  confirmButtonColor: '#3085d6',
              cancelButtonText: 'İptal',
  cancelButtonColor: '#d33',
  confirmButtonText: 'Sıfırla!'
}).then((result) => {
  if (result.isConfirmed) {
    Swal.fire({
       position: 'top-end',
  icon: 'success',
  title: 'Başarıyla bütün ayarların sıfırlandı.',
  showConfirmButton: false,
  timer: 2000
     
   } )
     window.location.href = `/dashboard/<%= guild.id %>/reset`;
  }
})
               }
          function myFunction2() {
          Swal.fire({
  title: 'Emin misin?',
  text: "Cidden beni atmak mı istiyorsun?.",
  icon: 'warning',
  showCancelButton: true,
  confirmButtonColor: '#3085d6',
            cancelButtonText: 'İptal',
  cancelButtonColor: '#d33',
  confirmButtonText: 'At!'
}).then((result) => {
  if (result.isConfirmed) {
    Swal.fire({
       position: 'top-end',
  icon: 'success',
  title: 'Başarıyla bot atıldı',
  showConfirmButton: false,
  timer: 2000
     
   } )
     window.location.href = ``;
  }
})
               }
            if(urlParams.has('error') && urlParams.get('error')){
            Swal.fire({
             icon: 'error',
             title: 'Tüh',
             text: 'Hata oluştu.',
             footer: ''
            }).then((result) => {
          if (result.isConfirmed) {
            window.location.href = `/dashboard/<%= guild.id %>/manage`;
          }    
        })
            }
     
           </script>
                   
